{
  "manifest_version": 2,
  "name": "notespass",
  "description": "",
  "version":"0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "main.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "notespass"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "contextMenus"
  ]
}
